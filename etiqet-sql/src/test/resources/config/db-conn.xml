<?xml version="1.0" encoding="UTF-8"?>
<DbConns>

    <DbConn name="default" driver_class="com.mysql.cj.jdbc.Driver" dialect="MYSQL">
        <subprotocol>jdbc:mysql://</subprotocol>
        <host>0.0.0.0</host>
        <port>3306</port>
        <db_name>db_name</db_name>
        <user>user</user>
        <password>pass</password>
    </DbConn>

    <DbConn name="mysql-example" driver_class="com.mysql.cj.jdbc.Driver" dialect="MYSQL">
        <subprotocol>jdbc:mysql://</subprotocol>
        <host>0.0.0.0</host>
        <port>3306</port>
        <db_name>employees</db_name>
        <user>root</user>
        <password>college</password>
    </DbConn>

    <DbConn name="tunnel-config-example" driver_class="com.mysql.cj.jdbc.Driver" dialect="MYSQL">
        <subprotocol>jdbc:mysql://</subprotocol>
        <host>127.0.0.1</host>
        <port>1234</port>
        <db_name>database</db_name>
        <user>user</user>
        <password>db-password</password>
        <SshTunnel>
            <username>remote-username</username>
            <local-port>1234</local-port>
            <remote-host>remote.host</remote-host>
            <remote-port>5555</remote-port>
            <password>ssh-password</password>
            <key>ssh-key</key>
        </SshTunnel>
    </DbConn>
</DbConns>
